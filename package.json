  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "yarn start:dev:db && nest start --watch",
    "start:dev:db": "./src/scripts/start-db.sh",
    "start:prod": "node dist/main",
    "shell": "docker exec -it bonfire_db psql -U postgres bonfire_db",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:ocg": "nodemon --watch 'src/common/{utils, interceptors}/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/common/utils/test.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-typeorm.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n migration",
    "typeorm:migration:run": "npm run typeorm -- migration:run"
  },
